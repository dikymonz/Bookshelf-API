{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "implicit-arrow-linebreak":"off",
    "comma-dangle":"off",
    "function-paren-newline": "off",
    "object-property-newline" : "off",
    "max-len" : "off"
  }
}
